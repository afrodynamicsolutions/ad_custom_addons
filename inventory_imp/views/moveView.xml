<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="imp_tax_extend" model="ir.ui.view">
        <field name="name">IMP Tax Extension</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='date_deadline']" position="after">
                <field name="customs_id" optional="hide"/>
            </xpath>
        </field>
    </record>
</odoo>